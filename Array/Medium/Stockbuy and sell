class Solution {
    public int maxProfit(int[] prices) {
        int maxi = 0;
        int min = Integer.MAX_VALUE;
        for(int i=0;i<prices.length;i++)
        {
            min = Math.min(min,prices[i]);
            maxi = Math.max(maxi,prices[i]-min);
        }
        return maxi;
    }
}

